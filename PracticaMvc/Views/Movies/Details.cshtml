@model PracticaMvc.Models.MovieDB
<div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title" id="detallesPeliculaModalLabel">Detalles del Usuario</h5>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"
                onclick="window.location.href='/User'"></button>

        </div>
        <div class="modal-body">
            <div class="row">
                <div class="col-md-8">
                    <p><strong>Id:</strong>@Model.Id</p>
                    <p><strong>Title:</strong>@Model.Title</p>
                    <p><strong>Director:</strong>@Model.Director</p>
                    <p><strong>Date:</strong>@Model.Date</p>
                </div>
            </div>
        </div>
    </div>
</div>
